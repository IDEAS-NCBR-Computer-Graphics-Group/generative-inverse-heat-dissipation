    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Debugger: sample.py with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/sample.py",
            "console": "integratedTerminal",
            "args": [
                "--config",
                "configs/ffhq/img_size_128_maxblur128.py",
                "--workdir",
                "runs/ffhq/img_size_128_maxblur128",
                "--checkpoint",
                "0",
                "--batch_size",
                "9"
            ]
        },
